{
  "env": {
    "browser": false,
    "es2021": true,
    "node": true
  },
  "extends": "standard-with-typescript",
  "overrides": [],
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "prettier/prettier": "error",
  "rules": {
    "@typescript-eslint/no-misused-promises": "warn"
  }
}
